{
  "name": "ham-tools",
  "displayName": "ham-tools",
  "description": "Ham Tools for VSCode",
  "version": "0.2.5",
  "publisher": "prenaux",
  "author": {
    "name": "Pierre Renaux",
    "email": "pierre@talansoft.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/prenaux/ham",
  "repository": {
    "type": "git",
    "url": "https://github.com/prenaux/ham"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ham.swiper",
    "onCommand:ham.swiper.swiper-word-at-cursor"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ham.swiper",
        "title": "Swiper Search/Resume",
        "category": "Ham Tools"
      },
      {
        "command": "ham.swiper-word-at-cursor",
        "title": "Swiper Search at Cursor",
        "category": "Ham Tools"
      }
    ],
    "configuration": {
      "title": "Ham Swiper",
      "properties": {
        "ham.swiper.selectMatch": {
          "type": "boolean",
          "default": true,
          "description": "Select the picked match text by swiper"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  }
}
