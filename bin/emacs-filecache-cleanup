#!/bin/bash
export HAM_NO_VER_CHECK=1
if [[ -z "$HAM_HOME" ]]; then echo "E/HAM_HOME not set !"; exit 1; fi
. "$HAM_HOME/bin/ham-bash-setenv.sh"

FILECACHE=$(abspath "$HOME/.emacs-filecache")
DATE=`arch_date`

set -ex
cp "${FILECACHE}" "${FILECACHE}-bak-${DATE}"
emacs-filecache-utils -o "${FILECACHE}-${DATE}" -C "${FILECACHE}-bak-${DATE}" exists
cp "${FILECACHE}-${DATE}" "${FILECACHE}"
